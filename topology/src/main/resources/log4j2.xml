<configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="1)%-4r [%t] %-5p %c{1.} - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="org.apache.zookeeper" level="WARN"/>
        <Root level="WARN">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</configuration>